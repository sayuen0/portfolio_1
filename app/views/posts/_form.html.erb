<%= form_with(model: post ) do |form|  %>
  <p>
    <%= form.label :title %>
  </p>
  <p>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :body %>
  </p>
  <p>
    <%= form.text_area :body,size: "20x10"  %>
  </p>
  <p>
    <%= form.label :genre %>
  </p>
  <p>
    <%= form.text_field :genre %>
  </p>
  <%= form.submit "編集する" %>
<% end %>


